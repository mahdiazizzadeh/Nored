{
  "compilerOptions": {
    "target": "ES2016",
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "alwaysStrict": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "strict": true,
    "strictBindCallApply": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "rootDir": "./src",
    "outDir": "./build",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "baseUrl": "./",
    "paths": {
      "tests/*": [
        "./test/*"
      ],
    },
    "resolveJsonModule": true,
    "types": [
      "node",
    ],
    "declaration": true,
    "declarationDir": "./types",
    "declarationMap": true,
    "removeComments": true,
    "allowJs": false,
    "checkJs": false,
    "skipLibCheck": true
  },
  "exclude": [
    "./test",
    "./node_modules",
    "./build",
    "./src/redisWriterForTest.ts"
  ]
}